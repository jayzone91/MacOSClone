{
  "name": "macosclone",
  "version": "1.0.0",
  "description": "A MacOS Clone written in Nunjucks, SassyCSS and Vanilla JS\r Build with 11ty",
  "main": ".eleventy.js",
  "scripts": {
  "watch:sass": "sass --watch _src/sass:_site/css",
  "build:sass": "sass _src/sass:_site/css",
  "watch:eleventy": "eleventy --serve",
  "build:eleventy": "eleventy",
  "start": "npm-run-all build:sass --parallel watch:*",
  "build": "npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayzone91/MacOSClone.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jayzone91/MacOSClone/issues"
  },
  "homepage": "https://github.com/jayzone91/MacOSClone#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.40.0"
  }
}
